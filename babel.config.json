{
  "presets": [
    ["@babel/preset-env"]
  ],
  "plugins": [
    "syntax-dynamic-import",
    "@babel/plugin-transform-runtime"
  ],
  "env": {
    "browser": {
      "presets": [
        [
          "env", {
            "targets": {
              "browsers": ["last 2 versions", "safari >= 7"]
            }
          }
        ]
      ]
    },
    "node": {
      "presets": [
        [
          "env", {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  }
}
